@model Model.EF.About
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="sap_tabs">
    <div id="horizontalTab" style="display: block; width: 100%; margin: 0px; margin-top: 120px;">
        <ul class="resp-tabs-list">
            <li class="resp-tab-item resp-tab-active" aria-controls="tab_item-0" role="tab"><span>About</span></li>

            <div class="clearfix"></div>
        </ul>
        <div class="resp-tabs-container">
            @if (Model != null)
            {
                @Html.Raw(Model.Detail)
            }
        </div>
    </div>

</div>